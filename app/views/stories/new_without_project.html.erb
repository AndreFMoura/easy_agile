<% content_for :title, "New Story" %>
<% content_for :h1, "New Story" %>
<% breadcrumbs 'New Story' %>
<% form_for @story do |form| %>
  <%= form.error_messages %>
  <%= render :partial => 'stories/project_select' %>
  <%= render form %>
  <p class="submit">
  <%= form.submit 'Create Story' %>
  </p>
<% end %>
