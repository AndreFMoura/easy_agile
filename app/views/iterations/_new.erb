<% form_tag new_project_iteration_path(@project), :method => :get do -%>
  <p>
  <%= label_tag :project_id, "Plan an iteration for" %>
  <%= select_tag :project_id,
    options_from_collection_for_select(User.current.projects,
                                       :id,
                                       :name) %>
  <%= submit_tag "Go" %>
  </p>
<% end -%>
