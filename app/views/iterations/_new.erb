<% form_tag new_iteration_path, :method => :get do -%>
  <p>
  <%= label_tag :project_id, "Plan an iteration for" %>
  <%= select_tag :project_id, 
    options_from_collection_for_select(@current_organisation.projects, 
                                       :id, 
                                       :name) %>
  <%= submit_tag "Go" %>
  </p>
<% end -%>
