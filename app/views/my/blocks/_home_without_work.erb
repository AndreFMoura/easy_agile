<div id="getting_started" class="section_container first">
  <div class="section">
  <h2>Getting Started</h2>

  <ul>
    <% unless @user.projects.empty? -%>
    <li><%= link_to "Create a new story", new_story_path %></li>
    <li>
      <%= render :partial => 'iterations/new' %>
    </li>
    <% end -%>
  </ul>
  </div>
</div>

<% unless @user.active_iterations.empty? -%>
  <div id="iterations" class="section_container">
    <div class="section">
      <h2>Active Iterations</h2>
      <div class="active">
        <ol>
          <%= render @user.active_iterations, :width => 300 %>
        </ol>
      </div>
    </div>
  </div>
<% end -%>
